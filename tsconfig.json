{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "./dist/out/",
    "rootDir": "./src/",
    "jsx": "react-jsx",
    "incremental": false,
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
